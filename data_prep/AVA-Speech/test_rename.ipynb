    "import urllib\n",
    "import lxml\n",
    "from lxml import etree\n",
    "youtube = etree.HTML(urllib.urlopen(\"http://www.youtube.com/watch?v=5BDj0ow5hnA\").read()) \n",
    "video_title = youtube.xpath(\"//span[@id='eow-title']/@title\") \n",
    "print (video_title)"
